{
  "expo": {
    "name": "EasyRider",
    "slug": "EasyRider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_wheelonly.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "translucent": false,
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_wheelonly.png",
        "backgroundColor": "#EDF2F4"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_wheelonly.png",
          "imageWidth": 200,
          "backgroundColor": "#EDF2F4"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
